{
  "name": "@mogul/components",
  "version": "0.4.0",
  "description": "React UI components based on Mobx && Antd, help freshesx to easy build admin pages",
  "private": false,
  "license": "MIT",
  "repository": "https://github.com/freshesx/mogul.git",
  "author": "zhangqing <zhangqing332@live.com>",
  "main": "build/mogul.js",
  "jsnext:main": "build/mogul.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o docs",
    "clean:node_modules": "rimraf ./node_modules",
    "build": "rollup -c ./scripts/rollup.config.js",
    "test": "eslint ./src/index.js",
    "lint": "eslint ./src/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/freshesx/mogul/issues"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.5",
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/react": "^3.4.6",
    "@types/react": "^16.3.16",
    "@types/shelljs": "^0.8.0",
    "antd": "^3.4.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",

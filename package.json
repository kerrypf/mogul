    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "inquirer": "^6.0.0",
    "lint-staged": "^8.1.0",
    "mobx": "^5.1.2",
    "mobx-react": "^5.1.2",
    "node-fetch": "^2.3.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-tippy": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.12.1",
    "shelljs": "^0.8.2",
    "styled-components": "^3.3.2"
  },
  "dependencies": {
    "css-element-queries": "^1.1.1",
    "invariant": "^2.2.4",
    "joi-browser": "^13.0.1",
    "polished": "^1.9.3",
    "popper.js": "^1.14.4",
    "query-string": "5.1.1",
    "react-content-loader": "^3.1.2",
    "react-measure": "^2.1.2",
    "react-sortable-hoc": "^0.8.3",
    "react-spring": "6.1.6",
    "react-virtualized": "9.21.0",
    "styled-tools": "^0.6.0"
  },
  "peerDependencies": {
    "antd": "^3.0.0 || ^2.0.0",
    "mobx": "^ 4.3.0 || ^5.0.0",
    "mobx-react": "^5.0.0",
    "moment": "*",
    "prop-types": "^15.0.0",
    "react": ">= 16.4.0",
    "react-dom": ">= 16.4.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "styled-components": "< 4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ],
    "typings/**/*.{d.ts}": [
      "prettier --write",
      "git add"
    ],
    ".storybook/**/*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "{README,CODE_OF_CONDUCT}.md": [
      "prettier --write",
      "git add"
    ]
  }
}
